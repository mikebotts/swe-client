<?xml version="1.0" encoding="UTF-8"?>
<sos:GetResultTemplateResponse
    xmlns:sos="http://www.opengis.net/sos/2.0"
    xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:xlink="http://www.w3.org/1999/xlink">
    <sos:resultStructure>
        <swe:DataRecord definition="http://sensorml.com/ont/swe/property/Location">
            <swe:field name="alt">
                <swe:Quantity axisID="h"
                    definition="http://sensorml.com/ont/swe/property/Altitude" referenceFrame="http://www.opengis.net/def/crs/EPSG/0/4979">
                    <swe:uom code="m"/>
                </swe:Quantity>
            </swe:field>
            <swe:field name="lat">
                <swe:Quantity axisID="Lat"
                    definition="http://sensorml.com/ont/swe/property/Latitude" referenceFrame="http://www.opengis.net/def/crs/EPSG/0/4979">
                    <swe:uom code="deg"/>
                </swe:Quantity>
            </swe:field>
            <swe:field name="lon">
                <swe:Quantity axisID="Long"
                    definition="http://sensorml.com/ont/swe/property/Longitude" referenceFrame="http://www.opengis.net/def/crs/EPSG/0/4979">
                    <swe:uom code="deg"/>
                </swe:Quantity>
            </swe:field>
            <swe:field name="time">
                <swe:Time definition="http://www.opengis.net/def/property/OGC/0/SamplingTime">
                    <swe:uom xlink:href="http://www.opengis.net/def/uom/ISO-8601/0/Gregorian"/>
                </swe:Time>
            </swe:field>
        </swe:DataRecord>
    </sos:resultStructure>
    <sos:resultEncoding>
        <swe:TextEncoding blockSeparator="&#xa;"
            collapseWhiteSpaces="true" decimalSeparator="." tokenSeparator=","/>
    </sos:resultEncoding>
</sos:GetResultTemplateResponse>
